        <required>false</required>
        <type>DateTime</type>
    </fields>
    <fields>
        <fullName>Fact_Location__c</fullName>
        <externalId>false</externalId>
        <formula>HYPERLINK( &quot;/apex/Event_Fact_Loc?id=&quot; + Id, &quot;карта&quot; )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Факт. место встречи (карта)</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
